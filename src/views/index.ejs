                // TODO: load search terms from database instead of hard coding them
                $("#term").typeahead({
                    hint: true,
                    highlight: true,
                    minLength: 1
                }, {
                    name: 'terms',
                    source: term
                });

                $("#match").typeahead({
                    hint: true,
                    highlight: true,
                    minLength: 1
                }, {
                    name: 'matches',
                    source: match
                });
